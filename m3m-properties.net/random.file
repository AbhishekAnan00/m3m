  vProject: "M3M The Cullinan Noida",
  vURL: window.location.href,
  thankspageurl: window.location.href + "thanks.htm"
};

/* ========== SLIDES INIT ========== */
(function(){
  const slidesEls = document.querySelectorAll('.slide');
  slidesEls.forEach((el, idx)=>{
    const bg = el.getAttribute('data-bg') || (SLIDES[idx] && SLIDES[idx].bg);
    if(bg) el.style.backgroundImage = `url('${bg}')`;
  });
  if(slidesEls.length) slidesEls[0].classList.add('active');

  let current=0,timer; const all = document.querySelectorAll('.slide');
  function show(i){ if(i<0) i=all.length-1; if(i>=all.length) i=0; all.forEach((s,idx)=> s.classList.toggle('active', idx===i)); current=i; }
  function start(){ timer=setInterval(()=>show(current+1), 4000); }
  function stop(){ clearInterval(timer); }
  start();
  document.getElementById('prevSlide').addEventListener('click', ()=>{ show(current-1); stop(); start(); });
  document.getElementById('nextSlide').addEventListener('click', ()=>{ show(current+1); stop(); start(); });
  const hero=document.querySelector('.hero'); hero.addEventListener('mouseenter', stop); hero.addEventListener('mouseleave', start);
})();

/* ========== MODAL SYSTEM ========== */
(function(){
  const backdrop = document.getElementById('modalBackdrop');
  const modalBox = document.getElementById('modalBox');

  function buildForm(fields){
    return fields.map(f=>`<input id="${f.id}" type="${f.type}" placeholder="${f.placeholder}">`).join('');
  }

  function openModal(cfg){
    const wrapper=document.createElement('div');
    wrapper.className='modal-inner';
    wrapper.innerHTML=`
      <div class="modal-left"><img src="${cfg.img}" alt="${cfg.title}"></div>
      <div class="modal-right">
        <div style="display:flex;justify-content:space-between;align-items:center;margin-bottom:8px">
          <h3>${cfg.title}</h3>
          <button class="close-x" data-close>&times;</button>
        </div>
        <form id="${cfg.id}_form">
          ${buildForm(cfg.formFields || [])}
          <button type="button" class="submit" id="${cfg.btnId}">Submit Now</button>
          <div style="font-size:12px;margin-top:8px;color:var(--muted)"><input type="checkbox" checked> I authorize company representatives to contact me.</div>
        </form>
      </div>`;
    modalBox.innerHTML=''; modalBox.appendChild(wrapper);
    backdrop.classList.add('show'); modalBox.classList.add('show');

    modalBox.querySelectorAll('[data-close]').forEach(b=> b.addEventListener('click', closeModal));
    backdrop.addEventListener('click', closeModal);

    const submitBtn=document.getElementById(cfg.btnId);
    if(submitBtn){
      submitBtn.addEventListener('click', function(){
        const FormInfo={
          SenderControlID: cfg.formFields[0]?.id || '',
          SenderControlMobileID: cfg.formFields[2]?.id || '',
          SenderControlEmailID: cfg.formFields[1]?.id || '',
          SenderControlMsgID: cfg.formFields[3]?.id || ''
        };
        try{
          if(typeof SubmitQueryData==='function'){ SubmitQueryData(AgentInfo, FormInfo); }
          else { showToast('Form submitted (demo).'); closeModal(); }
        }catch(e){ console.error(e); showToast('Form submitted (demo).'); closeModal(); }
      });
    }
  }

  function closeModal(){
    modalBox.classList.remove('show'); document.getElementById('modalBackdrop').classList.remove('show'); modalBox.innerHTML='';
  }

  // Delegate open
  document.addEventListener('click', function(e){
    const openTo = e.target.closest('[data-open]');
    if(openTo){
      const id = openTo.getAttribute('data-open').replace('#','');
      const cfg = MODALS.find(m=>m.id===id);
      if(cfg) openModal(cfg);
      e.preventDefault();
    }
  });

  // Auto open onload (Independence popup)
  window.addEventListener('load', ()=> {
    const cfg = MODALS.find(m=> m.id==='onload2');
    if(cfg){
      setTimeout(()=> {
        openModal(cfg);
      }, 1200); // 1.2s after page load
    }
  });

  // ESC to close
  document.addEventListener('keydown', function(e){
    if(e.key==='Escape'){
      if(backdrop && backdrop.classList.contains('show')){
        backdrop.classList.remove('show'); modalBox.classList.remove('show'); modalBox.innerHTML='';
      }
    }
  });
})();

